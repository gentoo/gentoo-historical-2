<?xml version="1.0"?>

	<logfile name="auth.log">
	  <match>auth,authpriv.*</match>
	</logfile>

	<logfile name="syslog" sync="On">
	  <match>*.*</match>
	  <match>auth,authpriv.none</match>
	</logfile>

	<logfile name="daemon.log" sync="On">
	  <match>daemon.*</match>
	</logfile>

	<logfile name="kern.log" sync="On">
	  <match>kern.*</match>
	</logfile>

	<logfile name="lpr.log" sync="On">
	  <match>lpr.*</match>
	</logfile>

	<logfile name="mail.log">
	  <match>mail.*</match>
	</logfile>

	<logfile name="user.log" sync="On">
	  <match>user.*</match>
	</logfile>

	<logfile name="uucp.log" sync="On">
	  <match>uucp.*</match>
	</logfile>

	<logfile name="imapd.log">
	  <match>local6.debug</match>
	</logfile>

	<logfile name="debug" sync="On">
	  <match>*.=debug</match>
	  <match>auth.authpriv.none</match>
	  <match>news.none</match>
	  <match>mail.none</match>
	</logfile>

	<logfile name="messages" sync="On">
	  <match>*.=info</match>
	  <match>*.=notice</match>
	  <match>*.=warn</match>
	  <match>auth.authpriv.none</match>
	  <match>cron,daemon.none</match>
	  <match>mail,news.none</match>
	</logfile>




