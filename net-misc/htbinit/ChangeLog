  +files/htb.init_find_fix.patch, +files/htb_0.8.5_esfq.diff,
  +files/htbinit.rc, +files/prio_rule.patch, +files/timecheck_fix.patch,
  -htbinit-0.8.4.ebuild, htbinit-0.8.5.ebuild, +htbinit-0.8.5-r1.ebuild:
