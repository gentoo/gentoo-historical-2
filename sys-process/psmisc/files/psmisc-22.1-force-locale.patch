Make sure we use [a-z] in a C locale

http://bugs.gentoo.org/119847

--- src/Makefile.in
+++ src/Makefile.in
@@ -455,6 +455,7 @@
 
 
 signames.h: signames.c Makefile
+		export LC_ALL=C ; \
 		@CPP@ -dM $< |\
 		tr -s '\t ' ' ' | sort -n -k 3 | sed \
 	's:#define SIG\([A-Z]\+[0-9]*\) \([0-9]\+\) *\(\|/\*.*\)$$:{\ \2,"\1" },:p;d' | \
