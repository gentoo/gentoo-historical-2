@       IN      SOA     localhost. root.localhost.  (
                                      2002081601 ; Serial
                                      28800      ; Refresh
                                      14400      ; Retry
                                      604800     ; Expire - 1 week
                                      86400 )    ; Minimum
        IN      NS      localhost.
1       IN      PTR     localhost.
