W3MBOOKMARK=@cgibindir@/w3mbookmark-en
eval `locale`
locale=${LC_ALL:-$LANG}
case X"$locale" in
  Xja|Xja_JP|Xja_JP.*)
    [ -x @cgibindir@/w3mbookmark-ja ] \
      && W3MBOOKMARK=@cgibindir@/w3mbookmark-ja
    ;;
 *)
    ;;
esac
exec $W3MBOOKMARK "$@"
