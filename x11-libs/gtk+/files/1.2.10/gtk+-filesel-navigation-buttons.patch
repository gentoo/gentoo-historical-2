+   gtk_box_pack_start (GTK_BOX (hbox), label, FALSE, FALSE, 0);
+ 
+   gtk_widget_show_all (hbox);
+ 
+   gtk_container_add (GTK_CONTAINER (btn), hbox);
+ 
+   return btn;
+ }
