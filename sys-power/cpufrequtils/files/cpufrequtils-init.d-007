change() {
	ebegin "Running cpufreq-set ${opts}"
	/usr/libexec/cpufrequtils-change.sh "${@}"
	eend $?
}

start() {
	change "${START_OPTS}" ${SYSFS_EXTRA}
}

stop() {
	change "${STOP_OPTS}"
}
