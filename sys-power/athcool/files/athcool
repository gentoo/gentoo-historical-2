#!/sbin/runscript
# $Header: /var/cvsroot/gentoo-x86/sys-power/athcool/files/athcool,v 1.1.1.1 2005/11/30 09:42:39 chriswhite Exp $

start() {
	ebegin "Starting Athcool"
	/usr/sbin/athcool on &>/dev/null || return 1
	eend $?
}

stop() {
	ebegin "Stopping Athcool"
	/usr/sbin/athcool off &>/dev/null || return 1
	eend $?
}

