depend() {
	need net
}

start() {
	ebegin "Starting Win4Lin"
	/etc/rc.d/init.d/Win4Lin start > /dev/null
	eend $?
}

stop() {
	ebegin "Stopping Win4Lin"
	/etc/rc.d/init.d/Win4Lin stop > /dev/null
	eend $?
}

