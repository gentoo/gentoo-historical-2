+libcap.so*
+_makenames
diff --git a/pam_cap/.gitignore b/pam_cap/.gitignore
new file mode 100644
index 0000000..11806f5
--- /dev/null
+++ b/pam_cap/.gitignore
@@ -0,0 +1,2 @@
+pam_cap.so
+testcompile
diff --git a/progs/.gitignore b/progs/.gitignore
new file mode 100644
index 0000000..280c19e
--- /dev/null
+++ b/progs/.gitignore
@@ -0,0 +1,4 @@
+capsh
+getcap
+getpcaps
+setcap
