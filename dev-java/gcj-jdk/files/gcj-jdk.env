VERSION="GCJ @PV@"
JAVA_HOME=/usr/lib/@P@
JDK_HOME=/usr/lib/@P@
JAVAC=${JAVA_HOME}/bin/javac
PATH="${JAVA_HOME}/bin"
ROOTPATH="${JAVA_HOME}/bin"
LDPATH="${JAVA_HOME}/lib"
INFOPATH="${JAVA_HOME}/info"
MANPATH="${JAVA_HOME}/man"
PROVIDES_TYPE="JDK JRE"
