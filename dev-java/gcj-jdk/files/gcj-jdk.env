BOOTCLASSPATH="${JAVA_HOME}/jre/lib/rt.jar"
GENERATION="2"
ENV_VARS="JAVA_HOME JDK_HOME JAVAC PATH ROOTPATH LDPATH INFOPATH MANPATH"
