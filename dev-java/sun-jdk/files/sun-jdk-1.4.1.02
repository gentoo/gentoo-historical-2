VERSION="Sun JDK @PV@"
JAVA_HOME=/opt/@P@
CLASSPATH=.:/opt/@P@/jre/lib:/opt/@P@/lib/tools.jar
JDK_HOME=/opt/@P@
JAVAC=${JAVA_HOME}/bin/javac
ADDPATH="${JAVA_HOME}/bin:${JAVA_HOME}/jre/bin"
ADDLDPATH="/opt/@P@/jre/lib"

ENV_VARS="JAVA_HOME CLASSPATH JDK_HOME JAVAC ADDPATH ADDLDPATH"
