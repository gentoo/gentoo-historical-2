# which port is the LCD connected
port="ttyS0"

# what driver should be used
driver="MtxOrb"

# "C" (CPU usage)
# "G" (CPU Graph like Xload)
# "M" (Memory Usage)
# "X" (X-Load)
# "T" (Date, time, Uptime, OS version)
# "D" (Disk Stats)
# "B" (Battery Stats)
# "A" (About)
# for more clients see "LCDproc Homepage"
clients="C M T X"
