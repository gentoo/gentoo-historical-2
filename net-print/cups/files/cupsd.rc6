depend() {
	need net
}

start() {
	ebegin "Starting cupsd"
	start-stop-daemon --start --quiet --background --exec /usr/sbin/cupsd
	eend $?
}

stop() {
	ebegin "Stopping cupsd"
	start-stop-daemon --stop --quiet --exec /usr/sbin/cupsd
	eend $?
}
