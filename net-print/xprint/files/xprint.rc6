depend() {
	use cupsd
}

start() {
	ebegin "Starting xprint"
	/usr/sbin/xprint start > /dev/null
	eend $?
}

stop() {
	ebegin "Stopping xprint"
	/usr/sbin/xprint stop > /dev/null
	eend $?
}
