depend() {
	use cups
}

start() {
	ebegin "Starting mtinkd"
	/usr/sbin/mtinkd start ${DEV_FILE} ${SPEC}
	eend $?
}

stop() {
	ebegin "Stopping mtinkd"
	/usr/sbin/mtinkd stop ${DEV_FILE} ${SPEC}
	eend $?
}
