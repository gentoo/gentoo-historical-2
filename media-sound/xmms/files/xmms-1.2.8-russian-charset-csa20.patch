+static int xmms_charset_number(char *c) {
+    int i;
+    if (!c) return 0;
+    for (i=0;charset_list[i];i++)
+	if (strcmp(charset_list[i], c)==0) return i;
+    return 0; /* Default, - if not found */
+}
