+            ki-=d;
+          }else{ //b>win[wi].b
+            ki+=d;
+          }
+        }else if(a[0]<koiptr->a){
+          ki-=d;
+        }else{ //a>win[wi].a
+          ki+=d;
+        }
+       }
+      }
+      if(!as){
+       if (ai>indexes2) ai-=d;
+       else {
+        altptr=enc_alt+ai-1;
+        if(a[0]==altptr->a){
+          if(a[1]==altptr->b){
+        	as=1;
+          }else if(a[1]<altptr->b){
+            ai-=d;
+          }else{ //b>win[wi].b
+            ai+=d;
+          }
+        }else if(a[0]<altptr->a){
+          ai-=d;
+        }else{ //a>win[wi].a
+          ai+=d;
+        }
+       }
+      }
+    }while(d);
