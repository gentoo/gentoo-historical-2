depend() {
	need localmount
}

start() {
	# check if this system uses udev
	[ -d /dev/.udev/ ] || return 0

	# store persistent-rules that got created while booting
	# when / was still read-only
	/lib/udev/move_tmp_persistent_rules.sh
}

stop() {
	:
}

