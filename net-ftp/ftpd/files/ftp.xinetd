# default: off
# $Header: /var/cvsroot/gentoo-x86/net-ftp/ftpd/files/ftp.xinetd,v 1.1.1.1 2005/11/30 09:36:25 chriswhite Exp $
# description: The netkit ftp daemon with optional SSL support.

service ftp
{
	socket_type		= stream
	protocol		= tcp
	wait			= no
	user			= root
	server			= /usr/bin/ftpd
	disable			= yes
}
