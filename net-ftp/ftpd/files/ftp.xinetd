# default: off
# $Header: /var/cvsroot/gentoo-x86/net-ftp/ftpd/files/ftp.xinetd,v 1.2 2004/08/14 13:41:20 swegener Exp $
# description: The netkit ftp daemon with optional SSL support.

service ftp
{
	socket_type		= stream
	protocol		= tcp
	wait			= no
	user			= root
	server			= /usr/bin/ftpd
	disable			= yes
}
