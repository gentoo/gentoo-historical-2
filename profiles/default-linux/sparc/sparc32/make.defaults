# 32bit kernel, 32bit userland
CHOST="sparc-unknown-linux-gnu"
PROFILE_ARCH="sparc"

# Just sparc32 binutils for linux-headers
CTARGETS_BINUTILS="sparc-unknown-linux-gnu"

# Compiler flags
CFLAGS="-O2 -pipe"
CXXFLAGS=${CFLAGS}

# Misc
FEATURES="sandbox ccache"
