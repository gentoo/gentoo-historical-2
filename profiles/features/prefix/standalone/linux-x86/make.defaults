ARCH="x86"
CHOST="i686-pc-linux-gnu"
# The base profile sets ACCEPT_KEYWORDS=x86 and we don't have that in prefix.
ACCEPT_KEYWORDS="-x86 ~x86-linux"

# multilib build stuff, single ABI (no multilib)
MULTILIB_ABIS="x86"
DEFAULT_ABI="x86"
ABI="x86"
IUSE_IMPLICIT="abi_x86_32"
LIBDIR_x86="lib"
