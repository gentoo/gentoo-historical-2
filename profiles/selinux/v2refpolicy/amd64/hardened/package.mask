# Hardened versions of gcc-4.0* through gcc-4.2* are not available.
=sys-devel/gcc-4.0*
=sys-devel/gcc-4.1*
=sys-devel/gcc-4.2*

# Mask non-hardened+non-testing gcc-4.3.x releases.
=sys-devel/gcc-4.3.2*

# hardened >=sys-devel/gcc-4.4.3-r3 >=gcc-4.4.4-r1 available.
=sys-devel/gcc-4.4.1*
=sys-devel/gcc-4.4.2*
=sys-devel/gcc-4.4.3-r2
=sys-devel/gcc-4.4.4
