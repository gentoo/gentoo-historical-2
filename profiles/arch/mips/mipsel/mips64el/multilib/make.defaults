PROFILE_ARCH="mips64el"

CHOST="mips64el-unknown-linux-gnu"
CFLAGS="-O2 -pipe"
CXXFLAGS="${CFLAGS}"

CFLAGS_o32="-mabi=32"
CHOST_o32="${CHOST}"
CDEFINE_o32="_MIPS_SIM == _ABIO32"
LIBDIR_o32="lib"

CFLAGS_n32="-mabi=n32"
CHOST_n32=${CHOST}
CDEFINE_n32="_MIPS_SIM == _ABIN32"
LIBDIR_n32="lib32"

CFLAGS_n64="-mabi=64"
CHOST_n64=${CHOST}
CDEFINE_n64="_MIPS_SIM == _ABI64"
LIBDIR_n64="lib64"

MULTILIB_ABIS="n64 n32 o32"
DEFAULT_ABI="n32"
