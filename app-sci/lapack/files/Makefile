	ar ruv liblapack.a $(OBJS)
	ranlib liblapack.a

shared: $(OBJS)
	cc -shared -Wl,-soname,liblapack.so.3 -o liblapack.so.3.0.3 *.o -L.. -lblas -lm -lc
